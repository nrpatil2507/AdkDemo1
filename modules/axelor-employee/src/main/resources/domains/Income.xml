<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="employee" package="com.axelor.employee.db" />

  <entity name="Income" cachable="true" table="income">

   <string name="paymentType" required="true" max="255" />
   <decimal name="amount"></decimal>
   <date name="paydate"></date>
   <one-to-one ref="Employee" name="employee" required="true"></one-to-one>
   <many-to-one name="employees" ref="Employee"></many-to-one>
  </entity>
</domain-models>
